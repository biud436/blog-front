        "next:start": "next start -p 8084",
        "next:lint": "next lint -p 8084",
        "test:generators": "ts-node ./internals/testing/generators/test-generators.ts",
        "checkTs": "tsc --noEmit",
        "eslint": "eslint --ext js,ts,tsx",
        "lint": "yarn run eslint src",
        "lint:fix": "yarn run eslint --fix src",
        "lint:css": "stylelint src/**/*.css",
        "generate": "plop --plopfile internals/generators/plopfile.ts",
        "cleanAndSetup": "ts-node ./internals/scripts/clean.ts",
        "prettify": "prettier --write"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "engines": {
        "node": ">=14.x"
    },
    "jest": {
        "collectCoverageFrom": [
            "src/**/*.{js,jsx,ts,tsx}",
            "!src/**/*/*.d.ts",
            "!src/**/*/Loadable.{js,jsx,ts,tsx}",
            "!src/**/*/messages.ts",
            "!src/**/*/types.ts",
            "!src/index.tsx"
        ],
        "coverageThreshold": {
            "global": {
                "branches": 90,
                "functions": 90,
                "lines": 90,
                "statements": 90
            }
        }
    },
