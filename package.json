    "scripts": {
        "start": "GENERATE_SOURCEMAP=false craco start",
        "build": "craco build",
        "test": "craco test",
        "eject": "react-scripts eject",
        "start:prod": "yarn run build && serve -s build",
        "test:generators": "ts-node ./internals/testing/generators/test-generators.ts",
        "checkTs": "tsc --noEmit",
        "eslint": "eslint --ext js,ts,tsx",
        "lint": "yarn run eslint src",
        "lint:fix": "yarn run eslint --fix src",
        "lint:css": "stylelint src/**/*.css",
        "generate": "plop --plopfile internals/generators/plopfile.ts",
        "cleanAndSetup": "ts-node ./internals/scripts/clean.ts",
        "prettify": "prettier --write",
        "extract-messages": "i18next-scanner --config=internals/extractMessages/i18next-scanner.config.js"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "engines": {
        "node": ">=14.x"
    },
    "jest": {
        "collectCoverageFrom": [
            "src/**/*.{js,jsx,ts,tsx}",
            "!src/**/*/*.d.ts",
            "!src/**/*/Loadable.{js,jsx,ts,tsx}",
            "!src/**/*/messages.ts",
            "!src/**/*/types.ts",
            "!src/index.tsx"
        ],
        "coverageThreshold": {
            "global": {
                "branches": 90,
                "functions": 90,
                "lines": 90,
                "statements": 90
            }
        }
    },
    "devDependencies": {
        "kakao.maps.d.ts": "^0.1.32",
        "prettier": "^2.6.2",
        "ts-node": "^10.8.0"
    }
}
